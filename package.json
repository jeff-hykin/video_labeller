{
    "name": "video_labeler",
    "version": "1.0.0",
    "author": "Jeff Hykin <jeff.hykin@gmail.com>",
    "description": "An electron-vue project",
    "license": "",
    "main": "./dist/electron/main.js",
    "scripts": {
        "build": "node .electron-vue/build.js && electron-builder",
        "build:dir": "node .electron-vue/build.js && electron-builder --dir",
        "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
        "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
        "dev": "node .electron-vue/dev-runner.js",
        "pack": "npm run pack:main && npm run pack:renderer",
        "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
        "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
        "postinstall": "",
        "package": "npx electron-packager . --no-prune --overwrite ; mv video_labeler-darwin-x64 app.nosync"
    },
    "vueElectronSettings": {
        "devPortNumber": 9080
    },
    "buildDependenciesFromSource": true,
    "nodeGypRebuild": true,
    "npmRebuild": false,
    "build": {
        "productName": "video_labeler",
        "appId": "jeffhykin.video_labeler",
        "directories": {
            "output": "build"
        },
        "files": [
            "dist/electron/**/*"
        ],
        "dmg": {
            "contents": [
                {
                    "x": 410,
                    "y": 150,
                    "type": "link",
                    "path": "/Applications"
                },
                {
                    "x": 130,
                    "y": 150,
                    "type": "file"
                }
            ]
        },
        "mac": {
            "icon": "build/icons/icon.icns"
        },
        "win": {
            "icon": "build/icons/icon.ico"
        },
        "linux": {
            "icon": "build/icons"
        }
    },
    "dependencies": {
        "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
        "@vue/babel-preset-jsx": "^1.1.2",
        "apexcharts": "^3.11.0",
        "axios": "^0.19.0",
        "babel-loader": "^8.0.6",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-minify": "^0.5.1",
        "cfonts": "^2.4.5",
        "chalk": "^3.0.0",
        "copy-webpack-plugin": "^5.1.1",
        "core-js": "^3.4.3",
        "css-baseline": "^1.1.10",
        "css-loader": "^3.3.2",
        "good-vue": "^1.3.1",
        "html-webpack-plugin": "^3.2.0",
        "js-yaml": "^3.13.1",
        "keen-ui": "^1.2.1",
        "lodash.merge": "^4.6.2",
        "mini-css-extract-plugin": "^0.8.0",
        "moment": "^2.24.0",
        "multispinner": "^0.2.1",
        "node-sass": "^4.13.1",
        "npm": "^6.13.4",
        "url-loader": "^3.0.0",
        "vue": "^2.6.11",
        "vue-apexcharts": "^1.5.1",
        "vue-electron": "^1.0.6",
        "vue-graph": "^0.8.7",
        "vue-json-pretty": "^1.6.2",
        "vue-loader": "^15.8.3",
        "vue-router": "^3.1.3",
        "vue-toasted": "^1.1.27",
        "webpack": "^4.41.3",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.9.0",
        "webpack-hot-middleware": "^2.25.0",
        "webpack-merge": "^4.2.2",
        "ytdl-core": "^1.0.3"
    },
    "devDependencies": {
        "electron": "^7.1.5",
        "electron-builder": "^21.2.0",
        "electron-debug": "^3.0.1",
        "electron-default-menu": "^1.0.1",
        "electron-devtools-installer": "^2.2.4",
        "electron-localshortcut": "^3.2.1",
        "electron-packager": "^14.1.1",
        "@vue/cli-plugin-babel": "^4.1.0",
        "@vue/cli-service": "^4.1.0",
        "es6-promise": "^4.2.8",
        "exports-loader": "^0.7.0",
        "fibers": "^4.0.2",
        "imports-loader": "^0.8.0",
        "sass": "^1.23.7",
        "sass-loader": "^8.0.0",
        "vue-template-compiler": "^2.6.11"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
